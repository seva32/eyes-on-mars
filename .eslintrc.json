{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["import", "unused-imports"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "unused-imports/no-unused-imports": "error",
    "import/order": [
      "error",
      {
        "alphabetize": { "order": "asc" },
        "newlines-between": "always"
      }
    ]
  }
}
